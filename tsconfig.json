{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "esModuleInterop":true,
        "moduleResolution":"Node",
        "sourceMap": true
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules","./dist"],
    "extends": "./paths.json"
  }